<h1>AjusteVenta</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Fk_venta</th>
            <td>{{ entity.fkventa }}</td>
        </tr>
        <tr>
            <th>Fecha_venta</th>
            <td>{{ entity.fechaventa|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Tipo_venta</th>
            <td>{{ entity.tipoventa }}</td>
        </tr>
        <tr>
            <th>Total_venta</th>
            <td>{{ entity.totalventa }}</td>
        </tr>
        <tr>
            <th>Total_venta_formal</th>
            <td>{{ entity.totalventaformal }}</td>
        </tr>
        <tr>
            <th>Total_venta_informal</th>
            <td>{{ entity.totalventainformal }}</td>
        </tr>
        <tr>
            <th>Total_iva_venta_formal</th>
            <td>{{ entity.totalivaventaformal }}</td>
        </tr>
        <tr>
            <th>Detalle_venta</th>
            <td>{{ entity.detalleventa }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('ajusteventa') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ajusteventa_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('ajusteventa_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
